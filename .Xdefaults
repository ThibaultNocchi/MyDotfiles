!URxvt*font: xft:UbuntuMono:size=12
URxvt*font: xft:SFMonoPowerline:size=11
URxvt*letterSpace: 1
URxvt*scrollBar: false
URxvt*internalBorder: 20

URxvt.perl-ext-common: default,clipboard,matcher
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.matcher.button: 1

! special
*.foreground:   #a8a19f
*.background:   #1b1918
*.cursorColor:  #a8a19f

! black
*.color0:       #1b1918
*.color8:       #766e6b

! red
*.color1:       #f22c40
*.color9:       #f22c40

! green
*.color2:       #5ab738
*.color10:      #5ab738

! yellow
*.color3:       #d5911a
*.color11:      #d5911a

! blue
*.color4:       #407ee7
*.color12:      #407ee7

! magenta
*.color5:       #6666ea
*.color13:      #6666ea

! cyan
*.color6:       #00ad9c
*.color14:      #00ad9c

! white
*.color7:       #a8a19f
*.color15:      #f1efee

